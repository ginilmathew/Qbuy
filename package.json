{
  "name": "diginest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "release": "adb reverse tcp:9090 tcp:9090 && npx react-native run-android --mode=release",
    "ios:green": "npx react-native run-ios --scheme QBuyGreenDev --mode DebugGreen",
    "ios:fashion": "npx react-native run-ios --scheme ShopqbuyDev --mode DebugFashion",
    "ios:panda": "npx react-native run-ios --scheme QBuyPandaDev --mode DebugPanda",
    "android:pandatest": "adb reverse tcp:9090 tcp:9090 && npx react-native run-android --mode=pandatestDebug --appIdSuffix=panda",
    "android:greentest": "adb reverse tcp:9090 tcp:9090 && npx react-native run-android --mode=greentestDebug --appIdSuffix=green",
    "android:fashiontest": "adb reverse tcp:9090 tcp:9090 && npx react-native run-android --mode=fashiontestDebug --appIdSuffix=fashion",
    "android:pandatestrelease": "adb reverse tcp:9090 tcp:9090 && npx react-native run-android --mode=pandatestRelease --appIdSuffix=panda",
    "android:greentestrelease": "adb reverse tcp:9090 tcp:9090 && npx react-native run-android --mode=greentestRelease --appIdSuffix=green",
    "android:fashiontestrelease": "adb reverse tcp:9090 tcp:9090 && npx react-native run-android --mode=fashiontestRelease --appIdSuffix=fashion",
    "android:pandalivedebug": "adb reverse tcp:9090 tcp:9090 && npx react-native run-android --mode=pandaDebug --appIdSuffix=panda",
    "android:greenlivedebug": "adb reverse tcp:9090 tcp:9090 && npx react-native run-android --mode=greenDebug --appIdSuffix=green",
    "android:fashionlivedebug": "adb reverse tcp:9090 tcp:9090 && npx react-native run-android --mode=fashionDebug --appIdSuffix=fashion",
    "android:pandaliverelease": "adb reverse tcp:9090 tcp:9090 && npx react-native run-android --mode=pandaRelease --appIdSuffix=panda",
    "android:greenliverelease": "adb reverse tcp:9090 tcp:9090 && npx react-native run-android --mode=greenRelease --appIdSuffix=green",
    "android:fashionliverelease": "adb reverse tcp:9090 tcp:9090 && npx react-native run-android --mode=fashionRelease --appIdSuffix=fashion",
    "android:panadadevapk": "cd android && ./gradlew assemblePandatestRelease",
    "android:greendevapk": "cd android && ./gradlew assembleGreentestRelease",
    "android:fashiondevapk": "cd android && ./gradlew assembleFashiontestRelease",
    "android:panadaliveapk": "cd android && ./gradlew assemblePandaRelease",
    "android:greenliveapk": "cd android && ./gradlew assembleGreenRelease",
    "android:fashionliveapk": "cd android && ./gradlew assembleFashionRelease",
    "android:panadabundle": "cd android && ./gradlew bundlePandaRelease",
    "android:greenbundle": "cd android && ./gradlew bundleGreenRelease",
    "android:fashionbundle": "cd android && ./gradlew bundleFashionRelease"
